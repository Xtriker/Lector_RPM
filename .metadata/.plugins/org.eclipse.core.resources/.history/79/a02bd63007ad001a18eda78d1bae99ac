#include "app_Debounce.h"

uint8_t boton = 0,contador = 0;
uint8_t app_LecturaBoton(void)
{
		HAL_Delay(5);
		contador = contador + 1;
		boton = HAL_GPIO_ReadPin(Entrada_GPIO_Port, Entrada_Pin);
}

void app_Debounce(void)
{

	switch(boton)
	{
		case NO_PRESIONADO:
		{

		}break;
		case PRESIONADO:
		{

		}break;
		default:
		{

		}
	}
}

